Algoritmo "soma"

Var

X, Y, resultado : inteiro;


Inicio



      escreva("Escreva o primeiro valor da soma:")
      leia(X)
      escreva("Escreva o segundo valo para a soma:")
      leia(Y)
      
      limpatela
      
      resultado <- X + Y;
      
      escreval("O valor da sua soma é igual a:", resultado)


Fimalgoritmo